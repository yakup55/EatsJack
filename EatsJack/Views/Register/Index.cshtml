
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/EatsLayout.cshtml";
}


   
<div class="login-contect py-5">
    <div class="container py-xl-5 py-3">
        <div class="login-body">
            <div class="login p-4 mx-auto">
                <h5 class="text-center mb-4">Giriş Yap</h5>
                <form action="#" method="post">
                    <div class="form-group">
                        <label>Email Adresiniz</label>
                        <input type="text" class="form-control" name="RegisterMail" placeholder="" required="">
                      
                    </div>
                    <div class="form-group">
                        <label class="mb-2">Şifreniz</label>
                        <input type="password" class="form-control" name="RegisterPassword" placeholder="" required="">
                    </div>
                    <button type="submit" class="btn submit mb-4">Giriş Yap</button>
                    <p class="forgot-w3ls text-center mb-3">
                        <a href="#" class="text-da">Şifreni mi unuttun?</a>
                    </p>
                    <p class="account-w3ls text-center text-da">
                        Hesabın Yok mu?
                        <a href="/Register/Register">Kayıt Ol</a>
                    </p>
                    <p class="account-w3ls text-center text-da">
                        Yemek Eklemeye Ne Dersin?
                        <a href="/Register/RegisterChefs">Şef Ol</a>
                    </p>
                </form>
            </div>
        </div>
    </div>
</div>
